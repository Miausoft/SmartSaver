{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Localization": {
    "Culture": "lt-LT"
  },

  "Cookie": {
    "Name": "UserCookie",
    "LoginPath": "/Authentication/Login",
    "AccessDeniedPath": "/Home/Index",
    "MinutesToExpiration": 5
  },

  "Email": {
    "Host": "smtp.gmail.com",
    "Port": 587
  },

  "HandlingPaths": {
    "Error": "/Error",
    "StatusCode": "/Error/{0}"
  },

  "TemplatePaths": {
    "Email": "../SmartSaver.MVC/Templates/Email.cshtml"
  }
}
